#!/usr/bin/tclsh
# Sintaxis de una funci贸n: proc <nombre_funci贸n> <argumento>

set nombre "nada"

proc imprimir_nombre cadena {		;# La funci贸n se invoca mediante 
puts "Entonces su nombre es: $cadena"	;# imprimir_nombre <cadena>
}

puts -nonewline "Su nombre: "
flush stdout
gets stdin nombre

imprimir_nombre	$nombre		;#invocacion de la funci贸n
